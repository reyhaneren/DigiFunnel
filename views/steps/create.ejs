<script type="text/ng-template" id="create.html">
  <h1>Create new funnel</h1>
  <form name="createForm">
    <div class="form-group">
      <label for="funnelName">Funnel Name</label>
      <input type="text" class="form-control" id="funnelName" ng-model="funnel.name" required>
    </div>
    <div class="form-group">
      <label for="description">Description</label>
      <input type="text" class="form-control" id="description" ng-model="funnel.description">
    </div>
  </form>
</script>
